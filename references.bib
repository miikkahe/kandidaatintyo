keywords = {annova,design of experiments,facing operation,n ratio,orthogonal array,s,taguchi method},
mendeley-groups = {kandityo},
number = {3},
pages = {13--19},
title = {{Application Of Taguchi Method For Optimization Of Process Parameters In Improving The Surface Roughness Of Lathe Facing Operation}},
volume = {1},
year = {2012}
}


